{
  "name": "react-super-select",
  "version": "0.5.16",
  "description": "A flexible replacement for the html select control built with React",
  "main": "./lib/react-super-select.js",
  "author": "Scotland Stephenson",
  "homepage": "https://github.com/alsoscotland/react-super-select",
  "repository": {
    "type": "git",
    "url": "https://github.com/alsoscotland/react-super-select.git"
  },
  "bugs": {
    "url": "https://github.com/alsoscotland/react-super-select/issues"
  },
  "files": [
    "lib/"
  ],
  "jest": {
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom"
    ],
    "testPathDirs": [
      "./src/"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "peerDependencies": {
    "classnames": "*",
    "lodash": "*",
    "react": ">=0.14.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^9.0.2",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "docco": "^0.7.0",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.2.2",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-bump": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^3.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-git": "^1.5.0",
    "gulp-less": "^3.0.3",
    "gulp-markdown-to-json": "^0.2.1",
    "gulp-minify-css": "^1.2.1",
    "gulp-recess": "^1.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.8.2",
    "less": "^2.5.3",
    "react": ">=0.14.0",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": ">=0.14.0",
    "run-sequence": "^1.1.4",
    "uglifyify": "^3.0.1"
  },
  "scripts": {
    "build": "npm run clean && npm run npm:build:css && npm run npm:babel:js && npm run docs:build-sequence",
    "clean": "rm -rf ./lib && mkdir ./lib",
    "docs:build-sequence": "npm run docs:clean && $(npm bin)/gulp build:docs-gulp-sequence",
    "docs:clean": "rm -rf ./src_docs/markdown/js/**/* docs_generated/*",
    "npm:babel:js": "$(npm bin)/babel src/react-super-select.js --out-file lib/react-super-select.js",
    "npm:build:css": "lessc ./src/react-super-select.less ./lib/react-super-select.css",
    "test": "$(npm bin)/jest"
  },
  "browserify-shim": {
    "classnames": "global:classNames",
    "lodash": "global:_",
    "react": "global:React"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "react",
    "react-component",
    "select",
    "UI-control",
    "UI",
    "accessible",
    "multiselect",
    "tagging",
    "tags"
  ],
  "license": "MIT"
}
